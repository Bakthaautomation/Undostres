<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Undostres App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">


	<listeners>
		<listener
			class-name="com.qa.undostres.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.undostres.listeners.TestAllureListener" />
		<listener
			class-name="com.qa.undostres.listeners.AnnotationTransformer" />
	</listeners>

	<test name="Login Page Test">
		<classes>
			<class name="com.qa.undostres.tests.LoginPageTest" />
		</classes>
	</test>
	
	<!-- <test name="Product Search Test">
